<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="domainSection" type="ConfigurationSettings.DomainSection, ExchangeDkimSigner"/>
    <sectionGroup name="customSection">
      <section name="general" type="ConfigurationSettings.General, ExchangeDkimSigner" allowLocation="true" allowDefinition="Everywhere"/>
    </sectionGroup>
  </configSections>
  <domainSection>
    <Domains>
      <Domain Domain="example.com" Selector="sel2012" PrivateKeyFile="keys/example.com.private" Rules="*"/>
      <Domain Domain="example.org" Selector="sel2013" PrivateKeyFile="keys/example.org.private" Rules="yahoo.com;yahoo.fr"/>
    </Domains>
  </domainSection>
  <customSection>
    <general LogLevel="3" HeadersToSign="From; Subject; To; Date; Message-ID;" Algorithm="RsaSha1"/>
  </customSection>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
